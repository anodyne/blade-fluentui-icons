{
    "name": "anodyne/blade-fluentui-icons",
    "description": "A package to easily make use of Heroicons in your Laravel Blade views.",
    "keywords": [
        "Blade",
        "Fluent UI System",
        "Laravel"
    ],
    "homepage": "https://github.com/anodyne/blade-fluentui-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Anodyne Productions"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.5",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "microsoft/fluentui-system-icons": "1.1.239",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "microsoft/fluentui-system-icons",
                "version": "1.1.239",
                "dist": {
                    "url": "https://github.com/microsoft/fluentui-system-icons/archive/refs/tags/1.1.239.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Anodyne\\FluentUiIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "update-icons": [
            "./bin/compile.sh ./vendor/microsoft/fluentui-system-icons/assets"
        ],
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anodyne\\FluentUiIcons\\BladeFluentUiIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}